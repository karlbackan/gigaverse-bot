# Gigaverse Bot Configuration

# Single Account Setup (for npm start)
JWT_TOKEN=your_jwt_token_here
WALLET_ADDRESS=your_wallet_address_here

# Multi-Account Setup (for npm run menu)
# Add up to 10 accounts
JWT_TOKEN_1=first_account_jwt_token_here
JWT_TOKEN_2=second_account_jwt_token_here
JWT_TOKEN_3=third_account_jwt_token_here
JWT_TOKEN_4=fourth_account_jwt_token_here
JWT_TOKEN_5=fifth_account_jwt_token_here

# Optional: Custom names for accounts
ACCOUNT_NAME_1=Account 1
ACCOUNT_NAME_2=Account 2
ACCOUNT_NAME_3=Account 3
ACCOUNT_NAME_4=Account 4
ACCOUNT_NAME_5=Account 5

# Bot Settings
DUNGEON_TYPE=UNDERHAUL
AUTO_SWITCH_UNDERHAUL=true  # Set to false to disable switching to Underhaul
ENERGY_THRESHOLD=40
REPAIR_THRESHOLD=30
MIN_ENERGY_TO_RUN=40

# Delays (in milliseconds)
ACTION_DELAY=2000
RUN_DELAY=5000
CHECK_INTERVAL=60000

# Debug
DEBUG=false